import java.lang.StringBuilder;
class Solution{

  static String toCamelCase(String s){
    
    String f="";
    
    for(int i=0;i<s.length();i++){
      
      if(s.charAt(i)=='-'||s.charAt(i)=='_'){
        i++;
        f=f+Character.toUpperCase(s.charAt(i));
      }
      else{
        f=f+s.charAt(i);
      }
      }
    return f;
  }
  
}
        
      
   
      
